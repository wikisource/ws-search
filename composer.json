{
    "name": "wikisource/ws-search",
    "description": "A search tool for all Wikisources.",
    "license": "GPL-3.0+",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/TestBase.php"]
    },
    "require": {
        "nikic/fast-route": "dev-master",
        "twig/twig": "1.*",
        "yohang/calendr": "1.*",
        "eloquent/asplode": "^2.0",
        "nayjest/str-case-converter": "^1.0",
        "addwiki/mediawiki-api-base": "2.*",
        "corneltek/getoptionkit": "2.*",
        "dflydev/dot-access-data": "^1.0",
        "symfony/dom-crawler": "^3.1",
        "magnusmanske/tooltranslate": "1.*"
    },
    "require-dev": {
        "symfony/var-dumper": "3.*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "magnusmanske/tooltranslate",
                "version": "1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://bitbucket.org/magnusmanske/tooltranslate.git",
                    "reference": "master"
                },
                "autoload": {
                    "files": ["public_html/tt.php"]
                }
            }
        }
    ]
}
